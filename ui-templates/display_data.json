{
    "display_data": {
        "python::module": "simulariumio",
        "python::object": "DisplayData",
        "parameters": {
            "name": {
                "name": "Name",
                "data_type": "text",
                "validation": "^[a-zA-Z0-9 #]*$",
                "help": "Please use only uppercase and lowercase letters, digits, and #",
                "description": "Provide display name (separate type and state with '#')",
                "required": true
            },
            "radius": {
                "name": "Radius",
                "data_type": "number",
                "validation": "^\\\\d*\\\\.?\\\\d+$",
                "help": "Please use only digits and decimals with no commas",
                "description": "Provide the scale for geometry or thickness of fiber",
                "required": false,
                "has_default": false
            },
            "display_type": {
                "name": "Display type",
                "data_type": "enum",
                "description": "type of geometry",
                "required": false,
                "has_default": false,
                "python::module": "simulariumio",
                "python::object": "DISPLAY_TYPE",
                "options": [
                    "SPHERE",
                    "PDB",
                    "OBJ",
                    "FIBER"
                ]
            },
            "url": {
                "name": "Geometry URL",
                "data_type": "url",
                "validation": "((([A-Za-z]{3,9}:(?:\\\\/\\\\/)?)(?:[\\\\-;:&=\\\\+\\\\$,\\\\w]+@)?[A-Za-z0-9\\\\.\\\\-]+|(?:www\\\\.|[\\\\-;:&=\\\\+\\\\$,\\\\w]+@)[A-Za-z0-9\\\\.\\\\-]+)((?:\\\\/[\\\\+~%\\\\/\\\\.\\\\w\\\\-_]*)?\\\\??(?:[\\\\-\\\\+=&;%@\\\\.\\\\w_]*)#?(?:[\\\\.\\\\!\\\\/\\\\\\w]*))?)",
                "help": "Please provide a valid URL",
                "description": "link to geometry file if display type is PDB or OBJ",
                "required": false,
                "has_default": false
            },
            "color": {
                "name": "Color",
                "data_type": "color",
                "validation": "\\\\B#([a-fA-F0-9]{6}|[a-fA-F0-9]{3})\\\\b",
                "help": "Please provide a hexadecimal value starting with '#'",
                "description": "color",
                "required": false,
                "has_default": false
            }
        }
    }
}