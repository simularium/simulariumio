{
    "meta_data": {
        "python::module": "simulariumio",
        "python::object": "MetaData",
        "parameters": {
            "size": {
                "name": "Box size",
                "data_type": "coordinates_xyz",
                "description": "Provide the size of the bounding box in XYZ",
                "required": false,
                "help": "If not provided, defaults to [100, 100, 100]"
            },
            "scaleFactor": {
                "name": "Scale factor",
                "data_type": "float",
                "description": "Provide a multiplier for 3D coordinates if the visualization is too large or too small",
                "required": false,
                "default": 1.0,
                "help": "If not provided, defaults to 1.0"
            },
            "cameraPosition": {
                "name": "Camera position",
                "data_type": "coordinates_xyz",
                "description": "position of the camera",
                "required": false,
                "default": {
                    "x": 0.0,
                    "y": 0.0,
                    "z": 120.0
                },
                "help": "If not provided, defaults to [0, 0, 120]"
            },
            "lookAtPosition": {
                "name": "Camera look at position",
                "data_type": "coordinates_xyz",
                "description": "target position to look at",
                "required": false,
                "default": {
                    "x": 0.0,
                    "y": 0.0,
                    "z": 0.0
                },
                "help": "If not provided, defaults to [0, 0, 0]"
            },
            "upVector": {
                "name": "Camera up vector",
                "data_type": "coordinates_xyz",
                "description": "up direction",
                "required": false,
                "default": {
                    "x": 0.0,
                    "y": 1.0,
                    "z": 0.0
                },
                "help": "If not provided, defaults to [0, 1, 0]"
            },
            "fovDegrees": {
                "name": "FOV (degrees)",
                "data_type": "float",
                "description": "field of view in degrees",
                "required": false,
                "default": 75.0,
                "help": "If not provided, defaults to 75"
            }
        }
    }
}