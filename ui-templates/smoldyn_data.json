{
    "smoldyn_data": {
        "python::module": "simulariumio.smoldyn",
        "python::object": "SmoldynData",
        "parameters": {
            "meta_data": {
                "name": "Metadata",
                "data_type": "meta_data",
                "description": "Provide metadata for this trajectory",
                "required": false,
                "has_default": false
            },
            "display_data": {
                "name": "Display data",
                "data_type": "dict",
                "description": "Provide display information for each particle type from Smoldyn",
                "required": false,
                "has_default": false,
                "length": 1,
                "extendible": true,
                "key_item": {
                    "name": "Particle name",
                    "data_type": "text",
                    "validation": "^[a-zA-Z0-9 ]*$",
                    "help": "Please use only uppercase and lowercase letters and digits",
                    "description": "Provide the name of the particle in Smoldyn",
                    "required": true
                },
                "value_item": {
                    "name": "Particle display data",
                    "data_type": "display_data",
                    "description": "Provide information about how to display this particle",
                    "required": true
                }
            },
            "time_units": {
                "name": "Time units",
                "data_type": "unit_data",
                "description": "Provide units for time values",
                "required": false,
                "has_default": false
            },
            "spatial_units": {
                "name": "Spatial units",
                "data_type": "unit_data",
                "description": "Provide units for 3D coordinates",
                "required": false,
                "has_default": false
            }
        }
    }
}