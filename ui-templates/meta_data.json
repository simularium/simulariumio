{
    "meta_data": {
        "python::module": "simulariumio",
        "python::object": "MetaData",
        "parameters": {
            "box_size": {
                "name": "Box size",
                "data_type": "coordinates_xyz",
                "description": "size of the bounding box",
                "required": false
            },
            "camera_defaults": {
                "name": "Camera defaults",
                "data_type": "camera_data",
                "description": "default view of the 3D scene",
                "required": false
            },
            "scale_factor": {
                "name": "Scale factor",
                "data_type": "number",
                "description": "a multiplier for 3D coordinates if the visualization is too large or too small",
                "required": false
            },
            "trajectory_title": {
                "name": "Trajectory title",
                "data_type": "text",
                "description": "a title for this trajectory, possibly describing parameters",
                "required": false
            },
            "model_meta_data": {
                "name": "Model metadata",
                "data_type": "model_meta_data",
                "description": "metadata for the model that was used to calculate this trajectory",
                "required": false
            }
        }
    }
}